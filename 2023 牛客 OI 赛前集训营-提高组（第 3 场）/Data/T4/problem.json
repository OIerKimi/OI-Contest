{
  "$schema": "https://aoi.zisu.dev/schemas/problem-config.json",
  "label": "uoj-compat",
  "submit": {
    "form": {
      "files": [
        {
          "path": ".metadata.json",
          "label": "Metadata",
          "type": {
            "metadata": {
              "items": [
                {
                  "key": "language",
                  "label": "Language",
                  "type": {
                    "select": {
                      "options": ["C++", "C++11", "C++14", "Python2", "Python3"]
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "path": "answer.code",
          "type": {
            "editor": {
              "language": "cpp"
            }
          },
          "label": "Source Code"
        }
      ]
    }
  },
  "judge": {
    "adapter": "uoj",
    "config": {}
  }
}
